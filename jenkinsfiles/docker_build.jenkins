pipeline {
  environment {
    registry = "roshepii/docker-test"
    registryCredential = ‘github’
  }
  agent any
  stages {
    stage('Cloning Git') {
      steps {
        git 'https://github.com/RoshepiiOlga/DevOps.git'
      }
    }
    stage('Building image') {
      steps{
        script {
          docker.build registry + ":$BUILD_NUMBER"
        }
      }
    }
  }
}